    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "electron": "^2.0.4",
    "file-loader": "^1.1.11",
    "jest": "^23.4.0",
    "node-sass": "^4.8.3",
    "proptypes": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
