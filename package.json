{
  "name": "YouTube",
  "version": "1.0.0",
  "description": "Youtube application",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "webpack-dev": "node_modules/.bin/webpack --mode development --watch",
    "webpack": "node_modules/.bin/webpack --mode production"
  },
  "jest": {
    "moduleNameMapper": {
